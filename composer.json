{
    "name": "rvxlab/hcaptcha",
    "description": "hCaptcha for Laravel",
    "license": "MIT",
    "keywords": [
        "hcaptcha",
        "captcha",
        "laravel"
    ],
    "authors": [
        {
            "name": "scyllaly",
            "email": "scyllaly@github.com"
        },
        {
            "name": "RVxLab",
            "email": "richard@rvx.works"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*",
        "illuminate/support": "^8.0 || ^9.0",
        "guzzlehttp/guzzle": "^7.4"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.23.1",
        "friendsofphp/php-cs-fixer": "^3.6",
        "nunomaduro/larastan": "^1.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5.10",
        "roave/security-advisories": "dev-latest",
        "orchestra/testbench": "^6.24 || ^7.0",
        "nunomaduro/collision": "^5.10"
    },
    "autoload": {
        "psr-4": {
            "Scyllaly\\HCaptcha\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Scyllaly\\HCaptcha\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "phpstan/extension-installer": true
        }
    },
    "suggest": {
        "laravelcollective/html": "Allows rendering the HCaptcha widget using the Form facade"
    },
    "extra": {
        "laravel": {
            "aliases": {
                "HCaptcha": "Scyllaly\\HCaptcha\\Facades\\HCaptcha"
            },
            "providers": [
                "Scyllaly\\HCaptcha\\HCaptchaServiceProvider"
            ]
        }
    }
}
